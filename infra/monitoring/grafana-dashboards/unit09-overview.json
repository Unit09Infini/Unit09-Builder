{
  "id": null,
  "uid": "unit09-overview",
  "title": "Unit09 Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "graph",
      "title": "API Request Rate",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "PROM_DS"
      },
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"unit09-api\"}[5m])",
          "legendFormat": "{{method}} {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "graph",
      "title": "Worker Job Throughput",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "PROM_DS"
      },
      "targets": [
        {
          "expr": "rate(unit09_worker_jobs_processed_total[5m])",
          "legendFormat": "jobs",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "graph",
      "title": "Error Rate",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "PROM_DS"
      },
      "targets": [
        {
          "expr": "rate(unit09_errors_total[5m])",
          "legendFormat": "errors",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ]
}